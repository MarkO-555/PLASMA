(DEFINE
  (LOOP (LAMBDA (I M FN)
    (COND ((AND (< I M) (FN I)),(LOOP (+ 1 I) M FN))
          (T,(EQ I M)))
  ))
  (LPRINT (LAMBDA (N)
    (PRINT N)
  ))
)

(PRINTLN 'TAIL)
(LOOP 1 100 LPRINT)
(PRINTLN 'FOR)
(FOR I 1 1 (< I 100) (PRINT I))
(PRINTLN 'WHILE)
(SETQ N 0)
(WHILE (< N 100) (PRINT N) (SETQ N (+ N 1)))
(PRINTLN 'UNTIL)
(SETQ N 1)
(UNTIL (> N 99) (PRINT N) (SETQ N (+ N 1)))
