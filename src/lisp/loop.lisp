(DEFINE
  (LOOP (LAMBDA (I M FN)
    (COND ((AND (< I M) (FN I)),(LOOP (+ 1 I) M FN))
          (T,(EQ I M)))
  ))
  (LPRINT (LAMBDA (N)
    (PRINT N)
  ))
)

(PRINTLN 'TAIL)
(LOOP 1 100 LPRINT)
(PRINT 'FOR)
(FOR I 1 1 (< I 100) (PRINT I))
(PRINT 'WHILE)
(CSETQ I 0)
(WHILE (< I 100) (PRINT I) (CSETQ I (+ I 1)))
(PRINT 'UNTIL)
(CSETQ I 1)
(UNTIL (> I 99) (PRINT I) (CSETQ I (+ I 1)))
